{
    "prompts": {
      "name": {
        "type": "string",
        "required": true,
        "label": "Project name"
      },
      "description": {
        "type": "string",
        "required": true,
        "label": "Project description",
        "default": "A Vue.js project"
      },
      "author": {
        "type": "string",
        "label": "Author"
      },
      "license": {
        "type": "string",
        "label": "License",
        "default": "MIT"
      },
      "mainFileName": {
        "type": "string",
        "required": true,
        "label": "the key main's value in package.json",
        "default": "build.min.js"
      },
      "sass": {
         "type": "confirm",
         "message": "Use sass?",
         "default": false
      },
      "private": {
         "type": "confirm",
         "message": "Is private?",
         "default": false
      }
    },
    "completeMessage": "{{#inPlace}}To get started:\n\n  npm install\n  npm run dev{{else}}To get started:\n\n  cd {{destDirName}}\n  npm install\n  npm run dev{{/inPlace}}"
  }